@model CmsA.Web.Models.Message.MessageModel
<h3>Message:  @Model.Name</h3>
<div class="card">
    <div class="card-body">


        <div class="row">
            <div class="col-md-6">
                <address>
                    <p class="font-weight-bold"> Time: </p>
                    <p> @Model.Time</p>

                    <p class="font-weight-bold"> Full Name: </p>
                    <p class="mb-2"> @Model.FullName </p>
                </address>
            </div>
            <div class="col-md-6">
                <address class="text-primary">
                    <p class="font-weight-bold"> E-mail: </p>
                    <p class="mb-2"> @Model.Email </p>

                      <p class="font-weight-bold"> Phone Number: </p>
                    <p class="mb-2"> @Model.PhoneNumber </p>


                </address>
            </div>
        </div>
    </div>
    <div class="card-body">
        <h4 class="card-title">Mesaj</h4>
        <p class="lead"> @Model.Content </p>
    </div>
</div>