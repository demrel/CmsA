<!--News One Start-->
<section class="news-one">
    <div class="container">
        <div class="section-title text-center">
            <h2 class="section-title__title">@Model.Project.Title</h2>
            <span class="section-title__tagline">@Html.Raw(Model.Project?.Description)</span>
        </div>
        <div class="row">
            @foreach (var item in Model.Project.LPosts)
            {
                    <div class="col-xl-4 col-lg-4 wow fadeInUp" data-wow-delay="0ms" data-wow-duration="1500ms">
                <!--News One Single-->
                <div class="news-one__single">
                    <div class="news-one__img">
                        <img src="~/images/post/@item.Image" alt="">
                        <a href="blog-details.html">
                            <span class="news-one__plus"></span>
                        </a>
                    </div>
                    <div class="news-one__content">
                        <h3 class="news-one__title">
                            <a href="/page/project/@item.Name">@item.Title</a>
                        </h3>
                      @Html.Raw(item.Description)
                        <a href="/page/project/@item.Name" class="news-one__btn">@Localizer["more"]</a>

                    </div>
                </div>
            </div>
            }
        


        </div>
    </div>
</section>
<!--News One End-->
